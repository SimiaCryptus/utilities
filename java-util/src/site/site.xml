<project name="java-util">
    <url>http://maven.apache.org/skins/maven-fluido-skin</url>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.6</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>false</sideBarEnabled>
            <googleSearch>
                <sitesearch/>
            </googleSearch>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <gitHub>
                <projectId>  ?  </projectId>
                <ribbonOrientation>left</ribbonOrientation>
                <ribbonColor>green</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>
    <body>
        <menu ref="reports" />
        <links>
            <item name="SimiaCryptus" href="http://simiacryptus.com"/>
        </links>
    </body>
</project>
